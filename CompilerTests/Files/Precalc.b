
: '['
for b in 8 16 32
do  echo "Esotope $b:" "$( tcc -run <( esotope-bfc -s$b $0 ) | cat -vt )"
done
echo "libbf char: $(libbf --compile-execute-char  $0)"

echo
for b in 8 16 32
do  echo "Tritium $b: $(bfi -b$b $0)"
done

exit 0
